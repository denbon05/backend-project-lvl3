{
    "env": {
        "browser": true,
				"es2021": true,
				"jest/globals": true
    },
    "extends": [
        "airbnb-base"
		],
		"plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
			"no-console": 0,
			"no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }],
			"import/extensions": 0
    }
}
